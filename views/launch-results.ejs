<!DOCTYPE html>
<html>

<head><%- include('./partials/head') %></head>

<body>
  <header>
    <%- include('./partials/header') %>
  </header>
  <main>
    <ul class="search_launch_parent">
      <% launchList.forEach(launch => { %>
      <li id="<%=launch.data_id%>" class="search_launch_container">
        <span class="search_provider-label">Provider: </span>
        <span class="search_mission-label">Mission: </span>

        <h3 class="search_launch_provider"><%=launch.launchProvider%></h3>
        <h3 class="search_launch_mission_name"><%=launch.missionName%></h3>
        <h3 class="search_date_info"><%=launch.launchDate%></h3>
        <h3 class="search_launch_status"><%=launch.statusName%></h3>
        <form action="/launch-results" method="POST">
          <input type="hidden" name="launch" value="<%= JSON.stringify(launch) %>"></input>
          <button class="search_track_launch" type="submit">Track This Launch</button>
        </form>
      </li>
      <% }) %>
    </ul>
  </main>
  <footer>
    <%- include('./partials/footer') %>
  </footer>
</body>

</html>